# Production Environment Variables for Backend
# DO NOT commit actual values - use AWS SSM Parameter Store or Secrets Manager

# Database
DATABASE_URL=postgresql://user:password@host:5432/clubcompass_prod

# Redis
REDIS_URL=redis://redis-host:6379

# Security - Generate with: openssl rand -hex 32
SECRET_KEY=CHANGE_THIS_IN_PRODUCTION_USE_STRONG_RANDOM_KEY
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS - Update with your actual frontend URL
ALLOWED_ORIGINS=https://clubcompass.bmsce.ac.in

# Email Configuration (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=

# Monitoring
SENTRY_DSN=
LOG_LEVEL=INFO

# Environment
ENVIRONMENT=production
